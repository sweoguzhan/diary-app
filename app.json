{
  "expo": {
    "name": "DiaryApp",
    "slug": "DiaryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Video Günlük uygulaması, video seçmeniz için fotoğraf kitaplığınıza erişim gerektirir.",
          "savePhotosPermission": "Video Günlük uygulaması, kırpılmış videoları kaydetmek için fotoğraf kitaplığınıza erişim gerektirir.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Video Günlük uygulaması, video seçmeniz için fotoğraf kitaplığınıza erişim gerektirir."
        }
      ],
      "expo-router"
    ],
    "scheme": "diaryapp",
    "experiments": {
      "typedRoutes": true
    }
  }
}
